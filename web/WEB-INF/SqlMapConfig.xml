<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMapConfig
        PUBLIC "-//ibatis.apache.org//DTD SQL Map Config 2.0//EN"
        "http://ibatis.apache.org/dtd/sql-map-config-2.dtd">

<sqlMapConfig>

    <!-- ibatis配置 -->
    <settings cacheModelsEnabled="true"
              lazyLoadingEnabled="true"
              enhancementEnabled="true"
              errorTracingEnabled="true"
              maxRequests="32"
              maxSessions="10"
              maxTransactions="5"
              useStatementNamespaces="true" />

    <!-- 手动配置ibatis的sql语句文件  -->
    <!-- 字典表 -->
    <sqlMap resource="com/xxhwap/sql/book_sql.xml"></sqlMap>
    <sqlMap resource="com/xxhwap/sql/school_card_sql.xml"></sqlMap>
    <sqlMap resource="com/xxhwap/sql/confManage_sql.xml"></sqlMap>
    <sqlMap resource="com/xxhwap/sql/picture.xml"></sqlMap>
    <sqlMap resource="com/xxhwap/sql/codeManage_sql.xml"></sqlMap>


</sqlMapConfig>
